{
	"name": "open-window-tab-context",
	"displayName": "Open Window Tab Context",
	"description": "Add an \"Open in new window\" option to tab context menu in editor.",
	"version": "1.0.0",
	"icon": "icon/icon.png",
	"publisher": "takkaO",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "open-window-tab-context.openFileInNewWindow",
				"title": "Open in new window"
			}
		],
		"menus": {
			"editor/title/context": [
				{
					"when": "resourceScheme == file",
					"command": "open-window-tab-context.openFileInNewWindow",
					"group": "navigation"
				}
			]
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/takkaO/VSCode-OpenWindowTabContext.git"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
