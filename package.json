{
	"name": "open-window-tab-context",
	"displayName": "Open Window Tab Context",
	"description": "Add an \"Open in new window\" option to tab context menu in editor.",
	"version": "1.4.0",
	"icon": "icon/icon.png",
	"publisher": "takkaO",
	"license": "MIT",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "open-window-tab-context.openFileInNewWindow",
				"title": "Open in new window"
			}
		],
		"menus": {
			"editor/title/context": [
				{
					"when": "resourceScheme == file || resourceScheme == untitled",
					"command": "open-window-tab-context.openFileInNewWindow",
					"group": "navigation"
				}
			]
		},
		"configuration": [
			{
				"title": "Open Window Tab Context",
				"properties": {
					"owtc.command": {
						"type": "string",
						"scope": "application",
						"default": "code",
						"description": "Command to open new window."
					}
				}
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/takkaO/VSCode-OpenWindowTabContext.git"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.5",
		"@types/vscode": "^1.45.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"tslint": "^6.1.2",
		"vscode-test": "^1.3.0"
	}
}
